          '</form>').submit();
    });

    $('[data-action="export-present"]').on('click', function (e) {
        var offer_type = $('#formExportPresent [name="offer_type_code"]:input').val();
        var realty_types = [];
        $('#formExportPresent [type="checkbox"]:input').each( function () {
          if(this.checked) {
